{
    "name": "mindy/application",
    "type": "library",
    "description": "Mindy Application Component",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "functions.php"
        ],
        "psr-4": {
            "Mindy\\Application\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Application\\Tests\\": "./Tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "~3.0|~4.0",
        "symfony/console": "~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
